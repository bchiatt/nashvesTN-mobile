html
  body
    ion-view(title='NashvesTN')
      ion-nav-buttons(side='left')
        button.button.button-icon.icon.ion-navicon(menu-toggle='left')
      ion-content.has-header

        label.item-input-wrapper
          i.icon.ion-ios7-search.placeholder-icon
          input(type='search' placeholder='Search')
        .list
          .item.item-thumbnail-left(ng-repeat='donee in donees', ng-click='details(donee._id')
            img(src='{{donee.photo}}')
            h2.padding {{donee.name}}
